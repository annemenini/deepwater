{
  "name": "deepwater-viewer",
  "version": "0.0.1",
  "description": "",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@tensorflow/tfjs": "0.13.3",
    "@tensorflow/tfjs-converter": "^0.6.7",
    "model": "^6.0.1"
  },
  "scripts": {
